{
  "name": "dripup",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parallelshell 'supervisor server.js' 'npm run watch'",
    "build": "npm run clean && npm run build:dir && parallelshell 'npm run build:assets' 'npm run build:js' 'npm run build:sass'",
    "build:dir": "mkdirp public/css public/js",
    "build:assets": "ncp assets public",
    "build:js": "browserify -e src/js/client.js -o public/js/client.bundle.js -t [ babelify ]",
    "build:sass": "node-sass --output-style compressed -o public/css src/sass --importer node_modules/node-sass-json-importer/dist/node-sass-json-importer.js && postcss -u autoprefixer -r public/css/**/*.css",
    "watch": "npm run watch:js & npm run watch:sass",
    "watch:js": "watchify -e src/js/client.js -o public/js/client.bundle.js -t [ babelify ] -v -d",
    "watch:sass": "onchange 'src/sass/**/*.scss' -- npm run build:sass",
    "clean": "rimraf public",
    "uglify": "uglifyjs public/js/client.bundle.js -o public/js/client.bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run lint:sass && npm run lint:js",
    "lint:js": "eslint src/js/**/*.js .eslintrc.js --no-ignore",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/sass/**/*.scss' -i 'src/sass/vendors/**/*' -v",
    "styleguide": "styleguidist server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuyoshiwada/dripup.git"
  },
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/dripup/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/dripup#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^6.0.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.8",
    "browserify": "^13.0.1",
    "envify": "^3.4.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.7.0",
    "node-sass-json-importer": "^2.0.0",
    "onchange": "^2.4.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.2",
    "react-motion": "^0.4.4",
    "react-styleguidist": "^2.3.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.2.0",
    "supervisor": "^0.10.0",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "b_": "^1.3.3",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.2",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.2.1",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "google-auth-library": "^0.9.8",
    "googleapis": "^7.1.0",
    "inline-style-prefixer": "^1.0.4",
    "iscroll": "^5.2.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.1",
    "method-override": "^2.3.6",
    "multer": "^1.1.0",
    "object-assign": "^4.1.0",
    "query-string": "^4.1.0",
    "rc-slider": "^3.7.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-textarea-autosize": "^4.0.2",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.4",
    "url-parse": "^1.1.1"
  }
}
