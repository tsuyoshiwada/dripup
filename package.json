    "postcss-cli": "^2.6.0",
    "react-addons-perf": "^15.3.1",
    "react-motion": "^0.4.4",
    "react-styleguidist": "^3.1.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.4",
