    "body-parser": "^1.15.2",
    "color-classifier": "0.0.1",
    "combokeys": "^2.4.6",
    "connect-mongo": "^1.3.2",
    "deep-equal": "^1.0.1",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "imagesloaded": "^4.1.1",
    "inline-style-prefixer": "^2.0.1",
