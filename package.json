{
  "name": "flamber",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all build --parallel server watch",
    "server": "supervisor -w lib lib/server",
    "clean": "rimraf lib public",
    "build": "npm-run-all clean build:dir --parallel build:assets build:js build:sass",
    "build:dir": "mkdirp lib public/css public/js",
    "build:assets": "ncp assets public",
    "build:js": "npm-run-all --parallel build:js:*",
    "build:js:client": "browserify -e src/js/client.js -o public/js/client.bundle.js -t [ babelify ]",
    "build:js:server": "ncp src/js/share-config.json lib/share-config.json && babel src/js --out-dir lib",
    "build:sass": "npm-run-all build:sass:*",
    "build:sass:compile": "node-sass --output-style compressed -o public/css src/sass --importer node_modules/node-sass-json-importer/dist/node-sass-json-importer.js",
    "build:sass:autoprefixer": "postcss -u autoprefixer -r public/css/**/*.css",
    "build:icon-index": "node ./scripts/icon-index.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "npm-run-all --parallel watch:js:*",
    "watch:js:client": "watchify -e src/js/client.js -o public/js/client.bundle.js -t [ babelify ] -v -d",
    "watch:js:server": "npm run build:js:server -- -w",
    "watch:sass": "watch 'npm run build:sass' src/sass/",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src/js/**/*.js .eslintrc.js --no-ignore",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/sass/**/*.scss' -i 'src/sass/vendors/**/*' -v",
    "test": "echo \"Error: no test specified\" && exit 1",
    "styleguide": "NODE_ENV=styleguide styleguidist server",
    "uglify": "uglifyjs public/js/client.bundle.js -o public/js/client.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuyoshiwada/flamber.git"
  },
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/flamber/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/flamber#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.15.0",
    "browserify": "^13.1.0",
    "envify": "^3.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.0",
    "glob": "^7.0.6",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.9.3",
